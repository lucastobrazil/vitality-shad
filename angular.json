{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "registry-ng": {
      "projectType": "application",
      "root": "registry-ng",
      "sourceRoot": "registry-ng/vitality",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/registry-ng",
            "index": "registry-ng/.storybook/index.html",
            "main": "registry-ng/.storybook/main.entry.ts",
            "tsConfig": "registry-ng/.storybook/tsconfig.json",
            "polyfills": [],
            "styles": ["./src/app/globals.css"]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "registry-ng/.storybook",
            "browserTarget": "registry-ng:build",
            "tsConfig": "registry-ng/.storybook/tsconfig.json",
            "port": 6006,
            "compodoc": false,
            "open": false
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "registry-ng/.storybook",
            "browserTarget": "registry-ng:build",
            "tsConfig": "registry-ng/.storybook/tsconfig.json",
            "outputDir": "storybook-static",
            "compodoc": false
          }
        }
      }
    }
  }
}
